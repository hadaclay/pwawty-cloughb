{
  "name": "nightlife-coord",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "watch": "nodemon ./start.js --ignore public/",
    "start": "node ./start.js",
    "assets": "webpack -w --display-max-modules 0",
    "dev": "concurrently \"npm run watch\" \"npm run assets\" "
  },
  "description": "View local bars and indicate if you're going",
  "author": "Clayton Hadaway <me@hadaclay.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "^1.17.2",
    "dompurify": "^0.9.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "mongoose": "^4.10.2",
    "pug": "^2.0.0-rc.1",
    "yelp-fusion": "^1.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0",
    "webpack": "^2.6.1"
  }
}
